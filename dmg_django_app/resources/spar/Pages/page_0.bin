<script type="text/javascript">
    var ddlSelector = "#store-fav-selector";
    var ddlWidth = "100%";

    var placeholderText = "Find a store";
    var totalResults = 0;
    var resultLimit = 2000;

    var searchTerm = "";
    var viewAllText = "View all results";

    function StoreFavForSpecials() {
        var storeID = $(ddlSelector).val();
        if (storeID == "" || storeID == "0") {
            return;
        }

        window.SPAR.StoreHistoryTracker.favouriteStore(storeID);
        location.reload();
    };

    function SearchRedirect() {
        console.log("Searching for: " + searchTerm);
        window.location.href = "/Store-Finder?searchTerm=" + searchTerm;
    };

    function formatStoreEntry(option) {
        totalResults++;
        if (totalResults > resultLimit) {
            return;
        }
        var storeName = $(option.element).data("storename");
        var address = $(option.element).data("address");
        var storeTypeImg = $(option.element).data("storetypeimg");

        if (!option.id) { return option.text; }
        var storeHtml = '<div class="select2-store-row"><span class="storeName">' + storeName + '</span>' + ' <img class="storeImage" src="' + storeTypeImg + '" /> ' + '<span class="storeAddress">' + address + '</span>' + '</div>';
        return storeHtml;
    };

    function storeSearch(params, data) {
        totalResults = 0; // reset total results

        if (typeof params.term !== 'undefined') {
            searchTerm = params.term;
        }

        $(".select2-view-all").show();
        // If there are no search terms, return all of the data
        if ($.trim(params.term) === '') { return data; }
        // Do not display the item if there is no 'text' property
        if (typeof data.text === 'undefined') { return null; }
        // `params.term` is the user's search term
        // `data.id` should be checked against
        // `data.text` should be checked against
        var q = params.term.toLowerCase();
        if (data.text.toLowerCase().indexOf(q) > -1 || data.id.toLowerCase().indexOf(q) > -1) {
            return $.extend({}, data, true);
        }
        // Return `null` if the term should not be displayed
        return null;
    };

    function LoadStoreSelector() {
        var select2DDL = $(ddlSelector).select2({
            placeholder: placeholderText,
            width: ddlWidth,
            allowClear: true,
            templateResult: formatStoreEntry,
            templateSelection: function (option) {
                if (option.id.length > 0) {
                    var storeName = $(option.element).data("storename");
                    return storeName;
                } else {
                    return option.text;
                }
            },
            escapeMarkup: function (m) {
                return m;
            },
            matcher: storeSearch,
            language: {
                noResults: function () {
                    $(".select2-view-all").hide();
                    return 'No matching stores';
                }
            }
        });

        select2DDL.on("select2:open", function (e) {
            var viewAllHtml = '<div class="select2-view-all" onclick="javascript:SearchRedirect();" title="' + viewAllText + '"><a href="javascript:void(0);">' + viewAllText + '</a></div>';
            $(".select2-results:not(:has(a))").append(viewAllHtml);
        });

        select2DDL.on("select2:close", function (e) {
            $(".select2-selection__placeholder").html(placeholderText);
        });

        select2DDL.on("select2:select", function (e) {
            var data = e.params.data;

            var currentUrl = window.location.href;
            var isHome = currentUrl.search(new RegExp("/home", "i"));
            if (isHome > 0) {
                window.location.href = "/Home/Store-View/" + $(data.element).data("alias");
                return;
            }

            window.SPAR.StoreHistoryTracker.favouriteStore(data.id);
            location.reload();
        });
    };

    $(function () {
        LoadStoreSelector();
    });
</script>
